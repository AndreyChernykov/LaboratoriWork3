public class Rifle extends Weapons{
	
	private String rifle;
	private int numbOfRound;
	
	public int getNumbOfRound() {
		return numbOfRound;
	}
	
	public void setNumbOfRound(int numbOfRound) {
		this.numbOfRound = numbOfRound;
	}
	
	public String getRifle() {
		return rifle;
	}
	
	public void setRifle(String rifle) {
		this.rifle = rifle;
	}
}
